{
    "name": "mak/merck-scraper",
    "require": {
	    "php": ">=7.4",
	    "illuminate/support": "^5.6",
	    "guzzlehttp/guzzle": "^7.3",
	    "mailjet/mailjet-apiv3-php": "^1.5",
	    "monolog/monolog": "^2.3",
	    "10quality/wp-query-builder": "^1.0",
        "nesbot/carbon": "^2.31",
        "php-http/guzzle7-adapter": "^1.0",
        "geocoder-php/free-geoip-provider": "^4.4",
        "php-http/message": "^1.12",
        "geocoder-php/google-maps-provider": "^4.6",
	    "ext-json": "*",
	    "vlucas/phpdotenv": "^3.0"
    },
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.5",
	    "roave/security-advisories": "dev-master"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-root-package-install": [
			"composer dump-autoload -o"
		],
		"post-root-package-update": [
			"composer dump-autoload -o"
		],
		"test": [
			"phpcs"
		],
		"compress": [
			"zip -r merck-scraper-wpml.zip ./ -x './node_modules/*' './.git/*' './yarn.lock' && mv merck-scraper-wpml.zip ../"
		]
	},
	"autoload": {
		"psr-4": {
			"Merck_Scraper\\": "../merck-scraper-wpml"
		}
	}
}
